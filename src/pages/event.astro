---
import Layout from '~/layouts/LandingLayout.astro';

import Hero2 from '~/components/widgets/Hero2.astro';
import CallToAction from '~/components/widgets/CallToAction.astro';
import Header from '~/components/widgets/Header.astro';
import { headerData } from '~/navigation';
import Event from '../components/Event.astro';
const metadata = {
  title: 'Sales Landing Page Demo',
};
const paper = {
  imageSrc: '/images/mfm.png',
  title: 'ICML MFM-EAI WorkShop',
  time:'Fri26 Jul',
  publication: 'ICML(International Conference on Machine Learning) 2024',
  location:'Messe Wien Exhibition Congress Center, Vienna'
};
---

<Layout metadata={metadata}>
  <!-- Hero2 Widget ******************* -->
  <Fragment slot="header">
    <Header {...headerData} isSticky showToggleTheme />
  </Fragment>

  <div style="display: flex;justify-content:center;margin:20px auto;">
    <Event {...paper} />
  </div>
</Layout>
